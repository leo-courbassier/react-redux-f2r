{
  "name": "f2r-app",
  "version": "0.1.0",
  "description": "Fit To Rent Tenant/Landlord Application",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist lint:tools",
    "start": "npm-run-all --parallel test:watch open:src",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint:tools": "eslint webpack.config.js tools",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "build:js": "babel-node tools/build.js",
    "prebuild": "npm run clean-dist && npm run build:html",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "cross-env NODE_ENV=test mocha --reporter progress --compilers js:babel-core/register --require ignore-styles",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Darren Mason <darrenjm@mac.com>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.6",
    "classnames": "2.2.5",
    "connect-history-api-fallback": "1.1.0",
    "es6-promise": "3.1.2",
    "estraverse-fb": "1.3.1",
    "file-loader": "0.9.0",
    "gemini-scrollbar": "1.4.2",
    "imports-loader": "0.6.5",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.15.0",
    "moment": "2.13.0",
    "object-assign": "4.0.1",
    "react": "15.3.1",
    "react-autosize-textarea": "0.3.3",
    "react-bootstrap": "0.30.3",
    "react-bootstrap-date-picker": "3.0.0",
    "react-bootstrap-switch": "3.4.5",
    "react-datetime": "2.3.2",
    "react-dial": "1.0.1",
    "react-dom": "15.3.1",
    "react-file-reader-input": "1.0.1",
    "react-ga": "2.1.0",
    "react-gemini-scrollbar": "2.1.5",
    "react-icons": "2.2.1",
    "react-mixin": "3.0.5",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-bootstrap": "0.22.1",
    "react-router-redux": "4.0.5",
    "react-script-loader": "0.0.1",
    "redux": "3.5.1",
    "redux-form": "6.0.2",
    "redux-form-schema": "0.0.3",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "smooth-scroll": "github:cferdinandi/smooth-scroll",
    "underscore": "1.8.3",
    "url-loader": "0.5.7",
    "validator": "5.3.0",
    "visibilityjs": "1.2.4"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.10.4",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.3",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.5.0",
    "browser-sync": "2.11.1",
    "chai": "3.4.1",
    "cheerio": "0.19.0",
    "colors": "1.1.2",
    "copy-webpack-plugin": "3.0.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "eslint": "3.3.1",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-react": "6.1.2",
    "extract-text-webpack-plugin": "1.0.1",
    "ignore-styles": "1.2.0",
    "mocha": "2.3.4",
    "node-sass": "3.4.2",
    "npm-run-all": "1.6.0",
    "rimraf": "2.5.0",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.11",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0",
    "yargs": "3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/bigwout31/f2r-web-app/"
  }
